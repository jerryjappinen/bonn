{
  "name": "bonn",
  "version": "0.11.2",
  "description": "Utility, style and component library for web development.",
  "private": false,
  "files": [
    ".github/*",
    "css/",
    "components/",
    "composables/",
    "eslint/",
    "experimentalComponents/",
    "icons/",
    "node/",
    "nuxt/",
    "prettier/",
    "scss/",
    "stores/",
    "utils/",
    "vercel/",
    "dist/",
    "composables.js",
    "icons.js",
    "iconIds.js",
    "iconSheet.js",
    "node.js",
    "utils.js",
    "README.md"
  ],
  "engines": {
    "node": "20.x"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf css dist",
    "build": "npm run build:scss && npm run build:optimizeIcons && npm run build:icons && npm run build:indexFiles",
    "build:indexFiles": "node ./build/indexFiles.js",
    "build:icons": "node ./build/icons.js",
    "build:optimizeIcons": "node ./build/optimizeIcons.js",
    "build:scss": "sass scss/index.scss css/index.css && sass scss/index.scss css/index.min.css --style=compressed",
    "lint": "eslint --ignore-path .gitignore \"**/*.{js,cjs,mjs,ts,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "dev": "vitest",
    "dev:components": "vitest --dir spec/components",
    "dev:nuxt": "vitest --dir spec/nuxt",
    "dev:utils": "vitest --dir spec/utils",
    "dev:vercel": "vitest --dir spec/vercel",
    "dev:scss": "sass spec/scss/index.spec.scss css/index.css --watch",
    "test": "npm run build && npm run test:bare",
    "test:bare": "npm run test:scss && npm run test:js",
    "test:js": "vitest run",
    "test:components": "vitest run --dir spec/components",
    "test:nuxt": "vitest run --dir spec/nuxt",
    "test:utils": "vitest run --dir spec/utils",
    "test:vercel": "vitest run --dir spec/vercel",
    "test:scss": "sass spec/scss/index.spec.scss"
  },
  "author": {
    "name": "Jerry JÃ¤ppinen",
    "url": "https://github.com/jerryjappinen"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerryjappinen/bonn.git"
  },
  "dependencies": {
    "@averjs/nuxt-compression": "^1.2.2",
    "@contentful/rich-text-types": "^15.14.1",
    "@graphcms/rich-text-html-renderer": "^0.3.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/robots": "^3.0.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@rollup/plugin-graphql": "^2.0.4",
    "@vanillaes/csv": "^3.0.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "contentful-rich-text-vue-renderer": "^3.1.0",
    "contentful-util": "^2.0.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "fuse.js": "^7.0.0",
    "graphql-request": "^6.1.0",
    "graphql-unwrap": "^1.0.1",
    "gravatar-url": "^4.0.1",
    "ky": "^1.1.3",
    "ky-universal": "^0.12.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.0.0",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "nodemailer": "^6.9.7",
    "nuxt": "^3.8.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "platform-detect": "^3.0.1",
    "prettier": "^3.1.1",
    "prism-es6": "^1.2.0",
    "qs": "^6.11.2",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "seedrandom": "^3.0.5",
    "vite-plugin-markdown": "^2.2.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue3-slider": "^1.9.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.3",
    "eslint-plugin-n": "^16.5.0",
    "glob": "^11.0.0",
    "happy-dom": "^12.10.3",
    "svgo": "^3.3.2",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.0"
  }
}
