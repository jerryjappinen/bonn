{
  "name": "bonn",
  "version": "0.2.11",
  "description": "Utility, style and component library for web development.",
  "private": false,
  "files": [
    "dist/*",
    "components/*",
    "experimentalComponents/*",
    "styledComponents/*",
    "composables/*",
    "icons/*",
    "eslint/*",
    "prettier/*",
    "nuxt/*",
    "scss/*",
    "stores/*",
    "util/*",
    "vercel/*",
    "README.md",
    ".editorconfig",
    ".nvmrc"
  ],
  "engines": {
    "node": "16.x"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:scss",
    "build:scss": "sass spec/scss/index.spec.scss dist/css/index.css && sass spec/scss/index.spec.scss dist/css/index.min.css --style=compressed",
    "lint": "eslint --ignore-path .gitignore \"**/*.{js,ts,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "dev": "vitest",
    "dev:components": "vitest --dir spec/components",
    "dev:nuxt": "vitest --dir spec/nuxt",
    "dev:util": "vitest --dir spec/util",
    "dev:vercel": "vitest --dir spec/vercel",
    "dev:scss": "sass spec/scss/index.spec.scss dist/css/index.css --watch",
    "test": "npm run test:scss && npm run test:js",
    "test:js": "vitest run",
    "test:components": "vitest run --dir spec/components",
    "test:nuxt": "vitest run --dir spec/nuxt",
    "test:util": "vitest run --dir spec/util",
    "test:vercel": "vitest run --dir spec/vercel",
    "test:scss": "sass spec/scss/index.spec.scss"
  },
  "author": {
    "name": "Jerry JÃ¤ppinen",
    "url": "https://github.com/jerryjappinen"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerryjappinen/bonn.git"
  },
  "dependencies": {
    "@averjs/nuxt-compression": "^1.2.2",
    "@graphcms/rich-text-html-renderer": "^0.2.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@pinia/nuxt": "^0.4.3",
    "@rollup/plugin-graphql": "^2.0.0",
    "@vanillaes/csv": "^3.0.1",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-promise": "^6.0.1",
    "graphql-request": "^5.0.0",
    "graphql-unwrap": "^1.0.1",
    "gravatar-url": "^4.0.1",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "md5-es": "^1.8.2",
    "pinia-plugin-persistedstate": "^2.2.0",
    "platform-detect": "^3.0.1",
    "prettier": "^2.7.1",
    "prism-es6": "^1.2.0",
    "qs": "^6.11.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "seedrandom": "^3.0.5",
    "vite-plugin-markdown": "^2.1.0",
    "vite-svg-loader": "^3.6.0",
    "vue3-slider": "^1.8.0"
  },
  "peerDependencies": {
    "contentful-management": "^10.15.1",
    "date-fns": "^2.29.3",
    "fetch-deep-contentful-data": "^5.0.1",
    "fuse.js": "^6.6.2",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.0.0-rc.11",
    "vue": "^3.2.40"
  },
  "peerDependenciesMeta": {
    "contentful-management": {
      "optional": true
    },
    "date-fns": {
      "optional": true
    },
    "fetch-deep-contentful-data": {
      "optional": true
    },
    "fuse.js": {
      "optional": true
    },
    "nuxt": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/test-utils": "^2.1.0",
    "date-fns": "^2.29.3",
    "happy-dom": "^7.4.0",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.0.0-rc.11",
    "pinia": "^2.0.23",
    "vitest": "^0.24.0",
    "vue": "^3.2.40"
  }
}
